{
  "version": 2,
  "rewrites": [
    {
      "source": "/apollo",
      "destination": "/api/index"
    },
    {
      "source": "/apollo/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/graphql",
      "destination": "/api/index"
    },
    {
      "source": "/graphql/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/((?!assets/|favicon\\.ico).*)",
      "destination": "/api/index"
    }
  ]
}
