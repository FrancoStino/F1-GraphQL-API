{
  "version": 2,
  "rewrites": [
    {
      "source": "/apollo",
      "destination": "/api/index.ts"
    },
    {
      "source": "/apollo/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/graphql",
      "destination": "/api/index.ts"
    },
    {
      "source": "/graphql/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/((?!assets/).*)",
      "destination": "/api/index.ts"
    }
  ]
}
