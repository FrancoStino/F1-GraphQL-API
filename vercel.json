{
  "rewrites": [
    {
      "source": "/apollo",
      "destination": "/api/index"
    },
    {
      "source": "/apollo/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/graphql",
      "destination": "/api/index"
    },
    {
      "source": "/graphql/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/",
      "destination": "/api/index"
    }
  ]
}
