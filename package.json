{
  "name": "f1-graphql",
  "version": "1.4.14",
  "description": "A comprehensive and flexible GraphQL API that provides access to historical and real-time Formula 1 data.",
  "author": "Davide Ladisa",
  "main": "index.ts",
  "keywords": [
    "graphql",
    "f1",
    "prisma",
    "typescript",
    "javascript",
    "node",
    "gqloom",
    "apollo",
    "graphql-yoga"
  ],
  "license": "ISC",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "pull": "prisma db pull",
    "build": "cp prisma/schema.prisma.example prisma/schema.prisma && yarn pull && prisma-case-format --file=prisma/schema.prisma && prisma generate",
    "generate-schema.graphql": "tsx src/schema/schemaGenerator.ts",
    "vercel-build": "yarn build",
    "patch": "yarn version --patch",
    "minor": "yarn version --minor",
    "major": "yarn version --major"
  },
  "packageManager": "yarn@1.22.22",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "prisma": "^6.11.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@as-integrations/express5": "^1.1.2",
    "@gqloom/core": "^0.13.0",
    "@gqloom/prisma": "^0.12.2",
    "@graphql-yoga/render-graphiql": "^5.16.0",
    "@prisma/client": "^6.11.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.25.0",
    "graphql-yoga": "5.16.0",
    "prisma-case-format": "^2.2.1"
  }
}
